{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Action": [
        "s3:*",
        "events:*",
        "lambda:*",
        "codecommit:*",
        "codebuild:*",
        "codepipeline:*",
        "sns:*",
        "dynamodb:*",
        "rds:*",
        "amplify:*",
        "cloudfront:*",
        "appsync:*",
        "apigateway:*",
        "quicksight:*",
        "forecast:*",
        "lookoutequipment:*",
        "sagemaker:*",
        "grafana:*",
        "ses:*",
        "cloudformation:*"
      ],
      "Resource": "*"
    },
    {
      "Effect": "Deny",
      "Action": [
        "dynamodb:CreateTable",
        "rds:CreateDBInstance",
        "rds:ModifyDBInstance",
        "sagemaker:CreateEndpoint",
        "sagemaker:UpdateEndpoint"
      ],
      "Resource": "*"
    }
  ]
}
